extern fn printf(char *, int) int

fn square(int x) int {
	return x * x
}

fn main() {
	int a = 2

	a = 3 * 1 + 2

	printf("%d should be 5\n", a)

	a = 2 + 3 * 1

	printf("%d should be 5\n", a)

	a = 1 + 1 * 1 + 1 * 1 + 1 * 1 + 1 * 1

	printf("%d should be 5\n", a)

	printf("5*5*25*625 is: %d\n", square(square(a*a) ) )
}
